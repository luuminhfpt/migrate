{
    "name": "luuminhfpt/bigin-migrate",
    "description": "Bigin Migrate Lib",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "MinhLuu",
            "email": "luuminhfpt@gmail.com"
        }
    ],
    "homepage": "https://github.com/luuminhfpt/migrate",
    "keywords": ["Laravel", "Migrate"],
    "require": {
        "illuminate/support": "~8"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~5|~6"
    },
    "autoload": {
        "psr-4": {
            "Bigin\\Migrate\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bigin\\Migrate\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Bigin\\Migrate\\MigrateServiceProvider"
            ],
            "aliases": {
                "Migrate": "Bigin\\Migrate\\Facades\\Migrate"
            }
        }
    }
}
